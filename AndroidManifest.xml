<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.infirmaryhelper"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.infirmaryhelper.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.infirmaryhelper.ActivityMenu"
            android:label="@string/title_activity_activity_menu" >
        </activity>
        <activity
            android:name="com.example.infirmaryhelper.DBHelper"
            android:label="@string/title_activity_activity_sqlite" >
        </activity>
        <activity
            android:name="com.example.infirmaryhelper.DbConstants"
            android:label="@string/title_activity_db_constants" >
        </activity>
    </application>

</manifest>